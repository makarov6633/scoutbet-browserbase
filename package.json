{
  "name": "scoutbet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3002",
    "build": "next build",
    "start": "next start --port 3002",
    "lint": "next lint",
    "test:browserbase": "node -e \"require('./src/lib/browserbase.ts').testBrowserbaseConnection().then(console.log)\"",
    "test:perplexity": "node -e \"console.log('Testing Perplexity AI connection...')\"",
    "test:director": "node -e \"console.log('Testing Director.ai integration...')\"",
    "test:sports-data": "node -e \"console.log('Testing Sports Data Sources...')\"",
    "test:all": "node scripts/test-integrations.js",
    "test:api": "curl -X POST http://localhost:3002/api/test-integrations -H \"Content-Type: application/json\" -d '{\"testType\": \"full\"}'",
    "test:chat": "curl -X POST http://localhost:3002/api/chat -H \"Content-Type: application/json\" -d '{\"message\": \"Quero odds do Flamengo vs Palmeiras\"}'",
    "clean": "rimraf ./.next && rimraf ./node_modules && npm install",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "analyze": "npm run type-check && npm run lint",
    "setup": "npm install && npm run build",
    "deploy": "npm run build && npm start"
  },
  "dependencies": {
    "@ai-sdk/perplexity": "^1.1.9",
    "@browserbasehq/sdk": "^2.6.0",
    "@browserbasehq/stagehand": "^2.4.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.52.1",
    "@vitejs/plugin-react": "^4.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "lucide-react": "^0.526.0",
    "next": "15.4.4",
    "playwright": "^1.54.1",
    "puppeteer": "^24.15.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "vite": "^7.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "sports-betting",
    "ai",
    "browserbase",
    "perplexity",
    "director-ai",
    "value-betting",
    "arbitrage",
    "odds-comparison",
    "football-predictions",
    "sports-analytics"
  ],
  "author": "ScoutBet Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/scoutbet/scoutbet.git"
  },
  "bugs": {
    "url": "https://github.com/scoutbet/scoutbet/issues"
  },
  "homepage": "https://scoutbet.com"
}
